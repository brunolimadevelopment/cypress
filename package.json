{
  "name": "cypress-project",
  "version": "1.0.0",
  "description": "how to run Cypress tests on multiple environments based on config files per environment.",
  "scripts": {
    "cy": "cypress open",
    "test": "cypress run",
    "test:homolog": "cypress run --config baseUrl=urldehomologacao --env environment=homolog",
    "test:prod": "cypress run --config baseUrl=https://urldeproducao --env environment=prod",
    "dev": "nodemon server.js"
  },
  "keywords": [
    "cypress.io",
    "multiple configs per environment"
  ],
  "author": "Bruno Lima",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^10.11.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-json": "^3.1.0",
    "express": "^4.18.2",
    "nodemon": "^3.0.3",
    "pre-commit": "^1.2.2"
  }
}
